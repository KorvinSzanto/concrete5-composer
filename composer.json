{
  "name": "concrete5/composer",
  "description": "concrete5 composer based installation",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "dev",
  "version": "0.0.9",
  "require": {
    "concrete5/concrete5": "^8.0"
  },
  "scripts": {
    "post-update-cmd": [
      "VERSION=`composer show -VD | awk -F\" \" '{print $2}'`;mkdir -p updates/$VERSION && cp -af vendor/concrete5/concrete5/concrete updates/$VERSION/concrete"
    ],
    "post-install-cmd": [
      "cp -af vendor/concrete5/concrete5/concrete ./"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "8.x-dev"
    }
  }
}
